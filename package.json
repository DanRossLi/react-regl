{
  "name": "react-regl",
  "version": "1.0.0",
  "description": "A React \"Stack\" renderer for regl webGL",
  "main": "dist/bundle.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/budo ./example/src/index.js --live --open -- -t [ babelify --presets [ es2015 react stage-0 ] ]",
    "build": "./node_modules/.bin/browserify ./src/index.js -o ./dist/bundle.js -t [ babelify --presets [ es2015 react stage-0 ] ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevzettler/react-regl.git"
  },
  "keywords": [
    "WebGL",
    "regl",
    "react",
    "GLSL",
    "glslify",
    "scene",
    "graph"
  ],
  "author": "kevzettler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevzettler/react-regl/issues"
  },
  "homepage": "https://github.com/kevzettler/react-regl#readme",
  "dependencies": {
    "display-tree": "^1.1.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "regl": "^1.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "budo": "^10.0.3"
  }
}
