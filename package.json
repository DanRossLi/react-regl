{
  "name": "react-regl",
  "version": "1.1.8",
  "description": "A React \"Stack\" custom renderer for regl webGL",
  "main": "dist/index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "./node_modules/.bin/babel-tape-runner ./test/**/*-test.js",
    "start": "./node_modules/.bin/budo ./example/src/index.js --live --open -- -t [ babelify --presets [ es2015 react stage-0 ] ]",
    "build": "./node_modules/.bin/gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevzettler/react-regl.git"
  },
  "keywords": [
    "WebGL",
    "regl",
    "react",
    "GLSL",
    "glslify",
    "scene",
    "graph"
  ],
  "author": "kevzettler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevzettler/react-regl/issues"
  },
  "homepage": "https://github.com/kevzettler/react-regl#readme",
  "dependencies": {
    "prop-types": "^15.5.10",
    "regl": "^1.3.0",
    "scene-tree": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "https://registry.npmjs.org/babel-plugin-transform-react-jsx/-/babel-plugin-transform-react-jsx-6.24.1.tgz",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "browserify-global-shim": "^1.0.3",
    "budo": "^10.0.3",
    "derequire": "^2.0.6",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "sinon": "^2.2.0",
    "tape": "^4.6.3"
  }
}
